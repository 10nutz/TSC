::========================================================================================
call clean.bat
::========================================================================================
call build.bat
::========================================================================================
cd ../sim

::echo %1 %2 %3 %4 %5
if "%6" == "gui" (
    vsim -gui -do "do run.do %1 %2 %3 %4 %5 %7" 
) else (
    if "%6" == "console" (
        vsim -c -do "do run.do %1 %2 %3 %4 %5 %7" 
    ) else (
        echo "The parameter must be gui or console"
    )
)

cd ../tools
::vsim -c -do run.do